  "scripts": {
    "docker": "next start -p 80",
    "s3:build": "cross-env IS_S3=true next build",
    "build:standalone": "cross-env IS_STANDALONE=true next build",
    "lint": "eslint **/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix '**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "deploy-s3": "npm run build:standalone && aws s3 sync out s3://acre-intranet --profile serverless --delete && aws cloudfront create-invalidation --distribution-id E12QFZK6VJ3I5X --paths \"/*\" --output text",
    "build-docker": "sh startScript.sh build",
    "deploy-aws": "bash deployDocker.sh",
    "run-image": "bash startScript.sh docker",
    "build": "bash startScript.sh build",
    "next-build": "next build",
    "next-prod": "next start",
    "start": "bash startScript.sh start",
    "prod": "bash startScript.sh prod",
    "next-dev": "next dev",
    "dev": "bash startScript.sh dev",
    "postinstall": "prisma generate",
    "generate": "npx prisma generate",
    "push": "npx prisma db push",
    "seed": "node ./src/scripts/seed.ts"
  },
